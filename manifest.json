{
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://www.hotmixradio.fr/*"],
      "js": ["contentscript.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [],
  "browser_action": {
    "default_icon": "icon_24.png",
    "default_title": "Click to search currently playing track on Google Music"
  },
  "manifest_version": 2,
  "name": "Hotmixradio Track Search on Google Music",
  "icons": {
    "16": "icon_16.png",
    "24": "icon_24.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "version": "0.0.1"
}

